{
  "name": "myapp-monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "native": "cd apps/expo && yarn start",
    "watch": "yarn workspaces foreach -pi run watch",
    "web": "yarn build && cd apps/next && yarn next",
    "web:extract": "DISABLE_EXTRACTION=false yarn workspace next-app dev",
    "web:prod": "yarn workspace next-app build",
    "web:prod:serve": "yarn workspace next-app serve",
    "postinstall": "yarn build",
    "build": "yarn workspaces foreach --exclude next-app -pi run build",
    "upgrade:tamagui": "manypkg upgrade tamagui && manypkg upgrade @tamagui && manypkg upgrade tamagui-loader"
  },
  "resolutions": {
    "esbuild": "0.15.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-refresh": "^0.14.0",
    "react-native-svg": "13.4.0",
    "react-native-web": "0.18.9"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/material": "^5.11.7",
    "@mui/x-date-pickers": "^5.0.16",
    "@react-native-community/datetimepicker": "^6.7.3",
    "date-fns": "^2.29.3",
    "react-day-picker": "^8.4.1",
    "react-device-detect": "^2.2.2",
    "react-native-date-picker": "^4.2.6",
    "react-native-modal-datetime-picker": "^14.0.1"
  },
  "devDependencies": {
    "@jsonforms/core": "^3.0.0",
    "@jsonforms/material-renderers": "^3.0.0",
    "@jsonforms/react": "^3.0.0",
    "@manypkg/cli": "^0.19.1",
    "eslint": "^8.21.0",
    "lodash": "^4.17.21",
    "prettier": "^2.7.1",
    "turbo": "^1.4.2",
    "typescript": "^4.7.4"
  },
  "packageManager": "yarn@3.2.3"
}
